<%= simple_form_for(@supplier, html: { class: "form-horizontal" }) do |f| %>


  <%= f.input :cnpj %>


  <%= f.input :name %>


  <%= f.input :street %>


  <%= f.input :street_number %>


  <%= f.input :neighbourhood %>


  <%= f.input :postal_code %>


  <%= f.input :address_complement %>


  <%= f.input :city %>


  <%= f.input :landline %>


  <%= f.input :mobile %>


  <%= f.input :ie %>


  <%= button_tag(type: 'submit',
                class: 'btn btn-primary',
                style: 'margin-top: 10px;
                        margin-left: 175px;
                        width: 220px;
                        font-weight: bold; ') do %>
        cadastrar
        <i id="icon-loading"></i>
  <% end %>
<% end %>
