
<% if user_signed_in? %>


  <ul id="vertical_menu">
    
    <li class="header" style="<%= 'background-color: transparent; ' if filter_list_not_present %>" >
    </li>


    <li class="first_filter <%#= menu_active_link('tickets') %>" style="">
      <%= link_to content_tag(:i, '', class: "fa fa-list-ul", style: "font-size: 16px; "), budgets_path, rel: 'tooltip', title: (t :"activerecord.models.budget").pluralize %>
    </li>



    <li class="first_filter <%#= menu_active_link('tickets') %>" style="">
